<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">

    <EditTextPreference
        android:key="pref_key_country_code"
        android:defaultValue="@string/pref_country_code_default"
        android:title="@string/pref_country_code_title"
        android:summary="@string/pref_country_code_summary"/>


    <ListPreference
        android:key="pref_key_result_returned"
        android:title="@string/pref_results_returned_title"
        android:summary="@string/pref_results_returned_summary"
        android:entries="@array/pref_results_returned_entries"
        android:entryValues="@array/pref_results_returned_values"
        android:defaultValue="@string/pref_results_returned_default"/>

</PreferenceScreen>